    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@contentful/rich-text-plain-text-renderer": "^13.4.0",
    "@types/algoliasearch": "^3.34.3",
