    "contentful": "^7.9.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "datocms-client": "^2.0.8",
    "dotenv": "^8.1.0",
    "eslint": "^6.1.0",
